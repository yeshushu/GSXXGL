-->
 
  
   <property name="loginname" column="loginname" type="string"  maxlength="50" description="登录账户"/> 
   <property name="loginpass" column="loginpass" type="string"  maxlength="50" description="登录密码"/> 
   <property name="email" column="email" type="string"  maxlength="50" description="邮箱"/>  
   <property name="status" column="status" type="int" description="是否激活"/>   
